package com.goingbetter.soloLearning;

public class INTERTE {
	public static void main(Object... o) {
		for(int i=0;i<o.length;i++) {
			if(o[i] instanceof Elipse) {
				System.out.println("Objeto "+i+"es una elipse.");
			}else if(o[i] instanceof Circulo) {
				System.out.println("Objeto "+i+"es un ciruculo.");
			}else if(o[i] instanceof Figura) {
				System.out.println("Objeto "+i+"es una figura.");
			}else {
				System.out.println("No es una figura. ");
			}
		}
		
	}
}
